@article{inkelas_positional_2007,
	title = {Positional Neutralization: A Case Study from Child Language},
	volume = {83},
	issn = {0097-8507},
	url = {https://www.jstor.org/stable/40070963},
	shorttitle = {Positional Neutralization},
	abstract = {A new longitudinal diary study of a child (E) learning American English reveals two patterns of segmental neutralization: velar fronting, in which /k/ and /g/ are realized as [t] and [d], and lateral gliding, in which /l/ is realized as [j]. Both phenomena are restricted to prosodically strong positions, affecting only consonants in word-initial position or in the onsets of stressed syllables. An explanation for positional velar fronting that combines phonetic and grammatical considerations is proposed to account for the occurrence of the effect in children but not adults: the greater gestural magnitude of prosodically strong onsets in English interacts with the anatomy of the young child's vocal tract to produce coronalization of prosodically strong velars. E extended the resulting pattern to lateral gliding, which developed later and has similar grammatical conditioning but less direct phonetic motivation.},
	pages = {707--736},
	number = {4},
	journaltitle = {Language},
	author = {Inkelas, Sharon and Rose, Yvan},
	urldate = {2024-10-10},
